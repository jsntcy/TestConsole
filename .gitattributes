# These files are text and should be normalized, set normalization eol to crlf
*.cs        text diff=csharp eol=crlf
*.xaml      text eol=crlf
*.sln       text eol=crlf
*.proj      text eol=crlf
*.props     text eol=crlf
*.ccproj    text eol=crlf
*.csproj    text eol=crlf
*.cscfg     text eol=crlf
*.csdef     text eol=crlf
*.cspkg     text eol=crlf
*.config    text eol=crlf
*.msbuild   text eol=crlf
*.tt        text eol=crlf
*.ps1       text eol=crlf
*.cmd       text eol=crlf
*.md        text eol=crlf
*.txt       text eol=crlf
*.json      text eol=crlf
*.js        text eol=crlf
*.css       text eol=crlf
*.html      text eol=crlf
*.cshtml    text eol=crlf
*.cmd       text eol=crlf
*.xml       text eol=crlf
*.srx       text eol=crlf
*.ini       text eol=crlf
*.xsd       text eol=crlf
*.xsl       text eol=crlf
*.aspx      text eol=crlf
*.asax      text eol=crlf
*.resx      text eol=crlf
*.StyleCop  text eol=crlf
*.Targets   text eol=crlf
*.resources text eol=crlf

# Images should be treated as binary
# (binary is a macro for -text -diff)
*.png     binary
*.jpg     binary
*.ico     binary
*.exe     binary
*.chm     binary
*.zip     binary